{
  "name": "pricing-dashboard",
  "version": "1.0.0",
  "description": "Product pricing comparison dashboard",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "setup": "chmod +x scripts/setup.sh && ./scripts/setup.sh",
    "integration-test": "node scripts/integration-test.js",
    "db:setup": "mysql -u root -p < src/lib/database/schema.sql",
    "db:test": "node -e \"require('./src/lib/test-utils').testDatabaseConnection().then(r => console.log('DB Health:', r ? 'OK' : 'FAIL'))\""
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "mysql2": "^3.6.0",
    "puppeteer": "^21.0.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "uuid": "^9.0.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "@types/cheerio": "^0.22.0",
    "@types/uuid": "^9.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "@testing-library/react": "^13.0.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.0.0"
  },
  "keywords": [
    "pricing",
    "comparison",
    "web-scraping",
    "dashboard"
  ],
